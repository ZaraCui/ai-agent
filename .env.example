# Environment variables for Travel Planning Agent
# Flask configuration
FLASK_DEBUG=False
PORT=5000

# Google Maps API Key (for map visualization)
GOOGLE_MAPS_API_KEY=your-google-maps-api-key-here
GOOGLE_PLACES_API_KEY=your-google-places-api-key-here

# Backend API Base URL (for Vercel deployment)
API_BASE=https://travel-planning-agent.onrender.com

# CORS Origins (comma-separated)
CORS_ORIGINS=*

# 高德地图 Web 服务 API Key
GAODE_API_KEY=your-gaode-web-service-api-key-here

# Redis Cache Configuration
# Enable/disable Redis caching (True/False)
REDIS_ENABLED=False

# Redis server connection settings
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0
REDIS_SOCKET_TIMEOUT=5